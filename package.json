{
	"name": "vscode-sf",
	"displayName": "Sami's VSCode Extension",
	"description": "VSCode Extension tailored to yours truly's preferences.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:sf.openNote"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "sf.openNote",
				"title": "Open Note"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Sami's Extension",
			"properties": {
				"sf.notesDirectory": {
					"type": "string",
					"description": "Notes directory pathname",
					"default": "~/d/dev/notes/"
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"lodash": "^4.17.15"
	}
}

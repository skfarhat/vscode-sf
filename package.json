{
	"name": "vscode-ext",
	"displayName": "vscode-ext",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:vscode-ext.helloWorld",
		"onCommand:vscode-ext.openNote"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-ext.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "vscode-ext.openNote",
				"title": "Open Note"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Todo - Configuration",
			"properties": {
			  "browsableFilenames": {
				"type": "string",
				"description": "Todo file name. Other supported names are: `*.todo`, `*.todos`, `*.task`, `*.tasks`, `*.taskpaper` and `todolist.txt`",
				"default": "/Users/sami/d/dev/notes/current.md"
			  },
			  "sfext.notesDirectory": {
				"type": "string",
				"description": "Notes directory pathname",
				"default": "/Users/sami/d/dev/notes/"
			  }
			}
		  }
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"lodash": "^4.17.15"
	}
}
